<div class="jumbotron text-center">
    <div class="overlay"></div>
    <div class="background-image"></div>
    <div class="caption">
        <h1>Mongo Scraper</h1>
        <p>NPR Edition</p>
    </div>
</div>
<div class = "row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each article}}
            <div class="card-panel">
                <h4>{{this.title}}</h4>
                <a href="/readArticle/{{this_id}}" class="waves-effect waves-light btn" id="btnSeach" >Read Article</a>
            </div>
    </div>
</div>